{
    "name": "iloh/simple-html-dom-bundle",
    "description": "The maintained version of 'Bundle around Simple HTML DOM library'",
    "type": "symfony-bundle",
    "keywords": ["html", "dom", "php"],
    "homepage": "https://github.com/ilohpro/SimpleHtmlDomBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Edoardo Rivello",
            "email": "edoardo.rivello@gmail.com",
	        "role" : "Author original version"
        },
        {
            "name": "Damien AUDOUX",
            "email": "damien.audoux@iloh.fr",
	        "role": "maintener, developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "symfony/framework-bundle": "4.* || 5.*",
        "symfony/finder": "4.* || 5.*",
        "symfony/yaml": "4.* || 5.*",
        "symfony/form": "4.* || 5.*",
        "symfony/security-csrf": "4.* || 5.*",
        "symfony/validator": "4.* || 5.*",
        "doctrine/annotations": "1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "8.*",
        "symfony/browser-kit": "4.* || 5.*"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": { "Iloh\\SimpleHtmlDomBundle\\": ""},
        "classmap": [ "lib" ]
    }
}
